{
  "name": "worker",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "sh -ac 'set -a; [ -f ../../.env ] && . ../../.env; set +a; wrangler dev --port 8787'",
    "build": "sh -ac 'set -a; [ -f ../../.env ] && . ../../.env; set +a; wrangler deploy --dry-run'",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.756.0",
    "@aws-sdk/s3-request-presigner": "^3.756.0",
    "@bandlab/shared": "workspace:*",
    "@hono/zod-validator": "^0.7.6",
    "@supabase/supabase-js": "^2.49.1",
    "hono": "^4.7.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250224.0",
    "typescript": "^5.7.3",
    "wrangler": "^4.2.0"
  }
}
